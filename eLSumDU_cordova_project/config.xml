<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="DL.SumDU" version="0.1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>DL.SumDU</name>
  <description>Mobile application for Sumy State University e-Learning</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Kuzikov Borys</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.1" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Fullscreen" value="true" />
  <preference name="KeepRunning" value="False" />
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows" id="DL.SumDU" version="0.1.0.1">
      <vs:name>DL.SumDU</vs:name>
      <vs:certFile>CordovaApp.pfx</vs:certFile>
      <vs:packageOutputPath>$(Solutiondir)\AppPackages\</vs:packageOutputPath>
    </vs:platformSpecificWidget>
  </vs:platformSpecificValues>
</widget>